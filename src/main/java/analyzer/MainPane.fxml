<?xml version="1.0" encoding="UTF-8"?>

<!--导入JavaFXML类-->

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<!--布局控件BorderPane，fx:controller属性用于声明事件处理的Controller，值为Controller类的类全名-->
<!--xmlns用于声明默认命名空间，这里的声明随着你安装的Java JDK版本号的不同可以不同，但是最好不要比你安装的JDK版本高-->

<BorderPane prefHeight="323.0" prefWidth="470.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="analyzer.controller.MainPaneController">
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ImageView fitHeight="62.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true">
               <GridPane.margin>
                  <Insets bottom="60.0" left="80.0" />
               </GridPane.margin>
               <image>
                  <Image url="@../resources/assets/开始.png" />
               </image></ImageView>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="启动测试">
               <GridPane.margin>
                  <Insets left="60.0" top="60.0" />
               </GridPane.margin></Button>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="测试记录" GridPane.columnIndex="1">
               <GridPane.margin>
                  <Insets left="60.0" top="60.0" />
               </GridPane.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="项目管理" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets left="60.0" top="60.0" />
               </GridPane.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="系统设置" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets left="60.0" top="60.0" />
               </GridPane.margin>
            </Button>
            <ImageView fitHeight="62.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1">
               <image>
                  <Image url="@../resources/assets/会议记录.png" />
               </image>
               <GridPane.margin>
                  <Insets bottom="60.0" left="80.0" />
               </GridPane.margin>
            </ImageView>
            <ImageView fitHeight="80.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="1">
               <image>
                  <Image url="@../resources/assets/管理标准化.png" />
               </image>
               <GridPane.margin>
                  <Insets bottom="60.0" left="80.0" />
               </GridPane.margin>
            </ImageView>
            <ImageView fitHeight="62.0" fitWidth="67.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <GridPane.margin>
                  <Insets bottom="60.0" left="80.0" />
               </GridPane.margin>
               <image>
                  <Image url="@../resources/assets/设置.png" />
               </image>
            </ImageView>
         </children>
      </GridPane>
   </center>
</BorderPane>
